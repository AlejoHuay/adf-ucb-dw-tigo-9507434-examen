{
	"name": "df_gold_dim_fecha",
	"properties": {
		"folder": {
			"name": "GOLD"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_silver_DETALLES_VENTA",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "select1"
				},
				{
					"name": "select2"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "select3"
				}
			],
			"scriptLines": [
				"source(output(",
				"          SLS_ORD_NUM as string,",
				"          SLS_PRD_KEY as string,",
				"          SLS_CUST_ID as string,",
				"          SLS_ORDER_DT as string,",
				"          SLS_SHIP_DT as string,",
				"          SLS_SALES as integer,",
				"          SLS_QUANTITY as integer,",
				"          SLS_PRICE as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source1 select(mapColumn(",
				"          SLS_ORDER_DT,",
				"          SLS_SHIP_DT",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 select(mapColumn(",
				"          FechaOrden = SLS_ORDER_DT,",
				"          FechaEnvio = SLS_SHIP_DT",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"select2 derive(FechaOrden = toDate(FechaOrden, 'yyyyMMdd'),",
				"          FechaEnvio = toDate(FechaEnvio, 'yyyyMMdd')) ~> derivedColumn1",
				"derivedColumn1 select(mapColumn(",
				"          fecha = FechaOrden,",
				"          fecha = FechaEnvio",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select3"
			]
		}
	}
}