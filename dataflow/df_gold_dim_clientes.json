{
	"name": "df_gold_dim_clientes",
	"properties": {
		"folder": {
			"name": "GOLD"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_silver_CLIENTES",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_dim_CLIENTES",
						"type": "DatasetReference"
					},
					"name": "sink1"
				}
			],
			"transformations": [
				{
					"name": "select1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          CST_ID as integer,",
				"          CST_KEY as string,",
				"          CST_FIRSTNAME as string,",
				"          CST_LASTNAME as string,",
				"          CST_MARITAL_STATUS as string,",
				"          CST_GNRD as string,",
				"          CNTRY as string,",
				"          BDATE as date,",
				"          GEN as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"source1 select(mapColumn(",
				"          CST_KEY,",
				"          CST_FIRSTNAME,",
				"          CST_LASTNAME,",
				"          CST_MARITAL_STATUS,",
				"          CST_GNRD,",
				"          CNTRY,",
				"          BDATE,",
				"          GEN",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"select1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          CST_ID as string,",
				"          CST_FIRSTNAME as string,",
				"          CST_LASTNAME as string,",
				"          CST_MARITAL_STATUS as string,",
				"          CST_GNRD as string,",
				"          CNTRY as string,",
				"          BDATE as date,",
				"          GEN as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sink1"
			]
		}
	}
}